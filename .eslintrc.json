{
    "env": {
        "browser": true,
        "es6": true
    },
    "ignorePatterns": [
        "*.min.*"
    ],
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-unreachable": "off"
    }
}